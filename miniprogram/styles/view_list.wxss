.listItemTitle{
    /* top:100%; */
    color:#4169E1;
    width: 95%;
    height: 20%;
    font-size: 34rpx;
    font-weight: bold;
    text-align: center;
    box-sizing: border-box;
    border-bottom-style: solid;
    border-bottom-width: 1px;
    border-bottom-color: #D2D2D2;
}
.listItemType{
    /* top:100%; */
    width: 100%;
    height: 15%;
    font-size: 32rpx;
    /* align-items: center; */
    text-align: center;

    box-sizing: border-box;
    border-top-style: solid;
    border-top-width: 1px;
    border-top-color: #D2D2D2;
    border-bottom-style: solid;
    border-bottom-width: 1px;
    border-bottom-color: #D2D2D2;
}
.listItemDetail{
    width: 100%;
    height: 50%;
    font-size: 32rpx;
    align-items: center;
    overflow: scroll;

    box-sizing: border-box;
    border-bottom-style: solid;
    border-bottom-width: 1px;
    border-bottom-color: #D2D2D2;
}
.listItemBtns{
    height: 20%;
    width: 80%;
    display: flex;
}
.listItemBtn{
    display: flex;
    justify-content: center;
    width: 40% !important;
    align-items: center;
    font-size:34rpx;
    font-weight: bold;
    height: 20% !important;
}
.listDisplay{
    display: flex;
    flex-direction: column;
    align-items: center;
    height: 95%;
    bottom:0%;
    overflow: auto;
    /* box-sizing: border-box;
    border-style: solid;
    border-color: black;
    border-width: 1px; */
}