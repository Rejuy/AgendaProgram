.listCellDisplay{
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    align-items: center;
    height: 90%;
    width: 100%;
    /* height: 80%; */
    background-color: white;
    border-radius: 80rpx;


    /* box-sizing: border-box;
